'.source.js':
    'Call update':
        'prefix': 'pcupdate'
        'body': """
            ${1:serviceName}.update(
                vm.${2:object},
                function(value, responseHeaders) {
                    vm.${3:value}=value;
                    vm.success = 'OK';
                    vm.error = null;
                },
                function(error) {
                    vm.success = null;
                    vm.error = error;
                });
        """
