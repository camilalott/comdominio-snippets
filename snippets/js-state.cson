'.source.js':
    'State':
        'prefix': 'pcstate'
        'body': """
            (function() {
                'use strict';

                angular
                    .module('${1:module}')
                    .config(${2:StateConfig});

                ${2:StateConfig}.$inject = ['$stateProvider', 'USER_ROLES'];

                function ${2:StateConfig}($stateProvider, USER_ROLES) {
                    $stateProvider.state('${2:state}', {
                        url: '${3:url}',
                        views: {
                            'content@': {
                                templateUrl: '${4:viewTemplateUrl}',
                                controller: '${5:viewController}',
                                controllerAs: 'vm',
                            }
                        },
                        data: {
                            authorities: [
                                ${6:USER_ROLES.all}
                            ],
                            pageTitle: '${7:pageTitle}'
                        },

                    });
                }
            })();
        """
