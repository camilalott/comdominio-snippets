'.text.html':
    'Show JSON':
        'prefix': 'pcjson'
        'body': '<pre class="col-md-12">{{vm.${1:object} | json}}</pre>'
    'List page':
        'prefix':'pc-list-html'
        'body':"""
            <div class="portlet">
                <h4 class="portlet-title">
                <ol class="breadcrumb">
                    <li><a href="#/">Início</a></li>
                    <li class="active">${1:titulo}</li>
                </ol>
                </h4>
                <div class="portlet-body" >
                    <pc-error-alert error="vm.error" ng-show="vm.error" ></pc-error-alert>

                    <pc-data-table-remote
                        url="vm.getRemoteUrl()"
                        on-select="vm.onSelect(value)"
                        columns="vm.columns"
                        datatable-scope="vm.datatableScope"
                        datatable-control="vm.datatableControl"
                        search-filters-template="filters.html"
                        extra="extra.html">
                    </pc-data-table-remote>

                    <pc-confirm ng-if="vm.confirmControl" confirm-control="vm.confirmControl" message="{{vm.getConfirmMessage()}}" on-ok="vm.remove(data)"/>
                </div>
            </div>

            <script type="text/ng-template" id="extra.html">
                <a class="btn btn-info" href="#/${2:caminho}/cadastrar" title="Cadastrar">
                    <i class="fa fa-plus"></i> Cadastrar
                </a>
            </script>

            <script type="text/ng-template" id="actions-column.html">
                <div class="text-center">
                    <a class="btn btn-info" href="#/${2:caminho}/{{item.id}}" title="Visualizar">
                        <i class="fa fa-eye"></i>
                    </a>
                    <a class="btn btn-warning" href="#/${2:caminho}/{{item.id}}/editar" title="Editar">
                        <i class="fa fa-pencil"></i>
                    </a>
                    <a class="btn btn-danger" href="" ng-click="datatableScope.confirmDelete({{item.id}})" title="Remover">
                        <i class="fa fa-remove"></i>
                    </a>
                </div>
            </script>

            <script type="text/ng-template" id="filters.html">
                <div class="well form-datatable">
                    <form name="formFilter" role="form" novalidate show-validation class="form" >
                        <div ng-init="form = formFilter"></div>

                        <div class="row">
                            <div class="col-md-12">
                                <h4>Pesquisa</h4>
                            </div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-12">
                                <label>Critérios</label>
                            </div>
                            <div class="col-md-4">
                                <div class="btn-group btn-group-justified" role="group">
                                    <label class="btn btn-info" ng-model="vm.filters.criteriaInclude" uib-btn-radio="'ALL'">Todos</label>
                                    <label class="btn btn-info" ng-model="vm.filters.criteriaInclude" uib-btn-radio="'ANY'">Qualquer um</label>
                                </div>
                            </div>
                        </div>

                        <div class="row form-group">
                            <div class="col-md-3">
                                <pc-form-group label="Código" label-translate="Código">
                                <input type="text" class="form-control" name="entityId" ng-model="vm.filters.entityId" ui-number-mask="0"
                                    ui-keypress="{enter:'vm.datatableControl.applySearch()'}">
                                </pc-form-group>
                            </div>

                            <div class="col-md-3">
                                <pc-form-group label="Nome" label-translate="Nome">
                                <input type="text" class="form-control" name="name" ng-model="vm.filters.name"
                                    ui-keypress="{enter:'vm.datatableControl.applySearch()'}">
                                </pc-form-group>
                            </div>

                        </div>

                        <div class="modal-footer">
                            <button class="btn btn-secondary" ng-click="vm.datatableControl.applySearch()">
                                <i class="fa fa-search"></i> Pesquisar
                            </button>
                            <button class="btn btn-warning" ng-click="vm.datatableControl.cancelSearch()">
                                <i class="fa fa-ban"></i> Cancelar
                            </button>
                        </div>
                    </form>
                </div>
            </script>
        """
    'New Page':
        'prefix': 'pc-page'
        'body': """
            <div class="portlet">
                <h4 class="portlet-title">
                <ol class="breadcrumb">
                    <li><a href="#/">Início</a></li>
                    <li class="active">${1:titulo}</li>
                </ol>
                </h4>
                <div class="portlet-body">
                    <pc-error-alert error="vm.error" ng-show="vm.error" ></pc-error-alert>

                </div>
            </div>
        """
    'Modal':
        'prefix': 'pc-modal'
        'body': """
            <div class="modal">
                <div class="modal-header">
                    <h3 class="modal-title">${1:title}</h3>
                </div>
                <div class="modal-body">

                </div>
                <div class="modal-footer">
                    <button class="btn btn-primary" type="button" ng-click="vm.ok()">OK</button>
                    <button class="btn btn-warning" type="button" ng-click="vm.cancel()">Cancelar</button>
                </div>
            </div>
        """
    'Form':
        'prefix': 'pc-form'
        'body': """
            <form name="${1:formName}" role="form" class="form form-horizontal" novalidate show-validation >
                <div ng-init="vm.${1:formName} = ${1:formName}"></div>
                ${2}
            </form>
        """
    'Form group':
        'prefix': 'pc-form-group-input'
        'body': """
            <pc-form-group label="${1:label}" label-translate="${1:label}">
                <input type="text" class="form-control" name="${2:name}" ng-model="vm.${3:model}" />
            </pc-form-group>
        """
